{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "moon",
            "request": "launch",
            "type": "dart",
            "cwd": "apps/moon",
            "args": [
                "--flavor",
                "development",
                "--target",
                "lib/main.dart"
            ]
        },
        {
            "name": "moon debug",
            "request": "launch",
            "type": "dart",
            "cwd": "apps/moon",
            "flutterMode": "debug",
            "args": [
                "--target",
                "lib/main.dart",
                "-d",
                "chrome",
                "--web-renderer",
                "html",
                "--web-port",
                "8888"
            ]
        },
    ]
}